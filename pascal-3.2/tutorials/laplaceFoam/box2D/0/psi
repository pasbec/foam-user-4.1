        type            fixedValueCorrected;
        volFieldName    psi;
        value           uniform 0;
    }
    top
    {
        type            fixedGradient;
        gradient        uniform 0;
//         type            fixedValue;
//         value           uniform 0;
    }
    left
    {
        type            fixedGradient;
        gradient        uniform 500;
    }
    right
    {
        type            fixedGradient;
        gradient        uniform -500;
    }
}

