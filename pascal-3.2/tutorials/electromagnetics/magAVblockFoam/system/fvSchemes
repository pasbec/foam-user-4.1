laplacianSchemes
{
    default                           none;

//     laplacian(ARe)                    Gauss linear corrected; // linear is not used here
//     laplacian(AIm)                    Gauss linear corrected; // linear is not used here
//
//     laplacian(sigma,VRe)              Gauss linear corrected;
//     laplacian(sigma,VIm)              Gauss linear corrected;
//     laplacian(sigmaf,VRe)             Gauss linear corrected; // linear is not used here
//     laplacian(sigmaf,VIm)             Gauss linear corrected; // linear is not used here
// //     laplacian(sigma,VRe)              Gauss harmonic corrected;
// //     laplacian(sigma,VIm)              Gauss harmonic corrected;
// //     laplacian(sigmaf,VRe)             Gauss harmonic corrected; // harmonic is not used here
// //     laplacian(sigmaf,VIm)             Gauss harmonic corrected; // harmonic is not used here

//     laplacian(ARe)                    Gauss linear limited 0.5; // linear is not used here
//     laplacian(AIm)                    Gauss linear limited 0.5; // linear is not used here
//
//     laplacian(sigma,VRe)              Gauss linear limited 0.5;
//     laplacian(sigma,VIm)              Gauss linear limited 0.5;
//     laplacian(sigmaf,VRe)             Gauss linear limited 0.5; // linear is not used here
//     laplacian(sigmaf,VIm)             Gauss linear limited 0.5; // linear is not used here
// //     laplacian(sigma,VRe)              Gauss harmonic limited 0.5;
// //     laplacian(sigma,VIm)              Gauss harmonic limited 0.5;
// //     laplacian(sigmaf,VRe)             Gauss harmonic limited 0.5; // harmonic is not used here
// //     laplacian(sigmaf,VIm)             Gauss harmonic limited 0.5; // harmonic is not used here

    laplacian(ARe)                    Gauss linear uncorrected; // linear is not used here
    laplacian(AIm)                    Gauss linear uncorrected; // linear is not used here

    laplacian(sigma,VRe)              Gauss linear uncorrected;
    laplacian(sigma,VIm)              Gauss linear uncorrected;
    laplacian(sigmaf,VRe)             Gauss linear uncorrected; // linear is not used here
    laplacian(sigmaf,VIm)             Gauss linear uncorrected; // linear is not used here
//     laplacian(sigma,VRe)              Gauss harmonic uncorrected;
//     laplacian(sigma,VIm)              Gauss harmonic uncorrected;
//     laplacian(sigmaf,VRe)             Gauss harmonic uncorrected; // harmonic is not used here
//     laplacian(sigmaf,VIm)             Gauss harmonic uncorrected; // harmonic is not used here
}

