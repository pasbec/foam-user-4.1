FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      faSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default none;
}

gradSchemes
{
    default none;
    grad(Us) Gauss linear;
    grad((Us&faceAreaNormals)) Gauss linear;
}

divSchemes
{
    default none;
    div(Us) Gauss linear;
    div(faceAreaNormals) Gauss linear;
}

laplacianSchemes
{
    default none;
}

interpolationSchemes
{
    default none;
    interpolate(Us) linear;
    interpolate(deltaH) linear;
}

snGradSchemes
{
    default none;
}

fluxRequired
{
}

