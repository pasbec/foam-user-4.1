FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         newTnterTrackFoam;

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime             5;

deltaT              0.0005;

writeControl        runTime;

writeInterval       0.001;

cycleWrite          0;

writeFormat         ascii;

writePrecision      8;

writeCompression    compressed;

timeFormat          general;

timePrecision       6;

runTimeModifiable   yes;

