FoamFile
{
    version     2.0;
    format      ascii;
    class       edgeScalarField;
    object      contactAngle;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    fixedWall
    {
        type            calculated;
        value           uniform 90;
//         value           uniform 120;
    }
    "procBoundary.*"
    {
        type            processor;
        value           $internalField;
    }
}


