FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuSgs;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    trackedSurface
    {
        type            slip;
//         type            calculated;
//         value           $internalField;
    }
    movingWall
    {
        type            nuSgsWallFunction;
        value           $internalField;
    }
    fixedWall
    {
        type            nuSgsWallFunction;
        value           $internalField;
    }
    "procBoundary.*"
    {
        type            processor;
        value           $internalField;
    }
}

