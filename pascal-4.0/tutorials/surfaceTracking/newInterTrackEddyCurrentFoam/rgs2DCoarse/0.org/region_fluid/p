FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    trackedSurface
    {
        type            fixedValue;
        value           uniform 0;
    }
    movingWall
    {
        type            fixedGradient;
        gradient        uniform 0;
//         type            fixedFluxPressure;
//         rAU             rAU;
    }
    fixedWall
    {
        type            fixedGradient;
        gradient        uniform 0;
//         type            fixedFluxPressure;
//         rAU             rAU;
    }
    "procBoundary.*"
    {
        type            processor;
        value           $internalField;
    }
}

