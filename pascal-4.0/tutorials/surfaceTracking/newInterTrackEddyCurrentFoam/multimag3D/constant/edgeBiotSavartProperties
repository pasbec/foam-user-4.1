FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      edgeBiotSavartProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

nNonOrthogonalCorrectors    10;

// currentAmplitude            26.3158; // = 1000.0000 / (4*10-2)
currentAmplitude            8.4770; // = 322.125 / (4*10-2)


// We need a current of
//
// I = 322.125 A
//
// for achieving
//
// B=0.4216 mT
//
// at centroid of cylinder for
//
// Ta=10^5
//
// with
//
// Ta=sigma*omega*B^2*(H/2)^4/(2*rho*nu^2)
//

inductors
{
    coil0
    {
        file       "coil0.eMesh";
        reverse    false;
        current    $currentAmplitude;
        phase      0.0;
    }

    coil1
    {
        file       "coil1.eMesh";
        reverse    false;
        current    $currentAmplitude;
        phase      60.0;
    }

    coil2
    {
        file       "coil2.eMesh";
        reverse    false;
        current    $currentAmplitude;
        phase      120.0;
    }

    coil3
    {
        file       "coil3.eMesh";
        reverse    false;
        current    $currentAmplitude;
        phase      180.0;
    }

    coil4
    {
        file       "coil4.eMesh";
        reverse    false;
        current    $currentAmplitude;
        phase      240.0;
    }

    coil5
    {
        file       "coil5.eMesh";
        reverse    false;
        current    $currentAmplitude;
        phase      300.0;
    }
}

