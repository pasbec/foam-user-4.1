FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// ************************* SIMLUTAITON TIMES ***************************** //

application         newInterTrackEddyCurrentFoam;

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime             10.0;

// *************************** TIME STEPPING ******************************* //

deltaT              1;

adjustTimeStep      yes;

// maxCo               5.0;
maxCo               2.0;

maxDeltaT           2e-5;

// *************************** OUTPUT SETTINGS ***************************** //

writeControl        adjustableRunTime;
// writeControl        timeStep;

writeInterval       0.001;
// writeInterval       1;

purgeWrite          0;

cycleWrite          0;

writeFormat         binary; // ascii
// writeFormat         ascii;

writePrecision      8;

writeCompression    uncompressed;

timeFormat          general;

timePrecision       8;

// **************************** OTHER SETTINGS ***************************** //

