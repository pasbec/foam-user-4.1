FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phase1
{
    transportModel    Newtonian;
    nu                nu  [ 0 2 -1 0 0 0 0 ] 1e-6;
    rho               rho [ 1 -3  0 0 0 0 0 ] 1000;
}

phase2
{
    transportModel    Newtonian;
    nu                nu  [ 0 2 -1 0 0 0 0 ] 0;
    rho               rho [ 1 -3 0 0 0 0 0 ] 0;
}

sigma                 sigma [ 1 0 -2 0 0 0 0 ] 50e-3;

concentration
{
//     betaSigma         betaSigma [ 1 0 -2 0 0 0 0 ] 0;
    betaSigma         betaSigma [ 1 0 -2 0 0 0 0 ] -1e-2;
    cRefSigma         cRefSigma [ 0 0  0 0 0 0 0 ] 0.5;

    betaRho           betaRho [ 1 -3 0 0 0 0 0 ] -20;
    cRefRho           cRefRho [ 0  0 0 0 0 0 0 ] 0.5;
}

