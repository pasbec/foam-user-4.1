FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         decomposePar;

startFrom           startTime;

startTime           0.0;

stopAt              endTime;

endTime             1.0;

deltaT              1.0;

writeControl        timeStep;

writeInterval       1;

purgeWrite          0;

cycleWrite          0;

writeFormat         ascii;

writePrecision      8;

writeCompression    uncompressed;

timeFormat          general;

timePrecision       8;

runTimeModifiable   yes;

