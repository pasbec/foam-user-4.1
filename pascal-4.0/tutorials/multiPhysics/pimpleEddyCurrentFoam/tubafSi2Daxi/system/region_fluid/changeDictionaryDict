FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    boundary
    {
        topWall
        {
            type            directMappedWall;
            sampleMode      nearestPatchFace;
            sampleRegion    region_static;
            samplePatch     topWall;
            offset          (0 0 0);
        }

        sideWall
        {
            type            directMappedWall;
            sampleMode      nearestPatchFace;
            sampleRegion    region_static;
            samplePatch     sideWall;
            offset          (0 0 0);
        }

        cornerWall
        {
            type            directMappedWall;
            sampleMode      nearestPatchFace;
            sampleRegion    region_static;
            samplePatch     cornerWall;
            offset          (0 0 0);
        }

        bottomWall
        {
            type            directMappedWall;
            sampleMode      nearestPatchFace;
            sampleRegion    region_static;
            samplePatch     bottomWall;
            offset          (0 0 0);
        }
    }
}

