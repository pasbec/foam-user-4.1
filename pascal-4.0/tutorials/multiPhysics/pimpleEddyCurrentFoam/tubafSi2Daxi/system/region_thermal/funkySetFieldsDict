// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

materialProperties
(
    // Data from Vladi
    "rhoCp_siliconLiquid=2432235.66;" // = 2580*942.727 (rho * cp)
    "rhoCp_siliconSolid=1610000;"     // = 2300*700 (rho * cp)
    "lambda_siliconLiquid=66.448;"    // 2580*942.727*3.333e-7/0.0122 (rho * cp * nu / pr)
    "lambda_siliconSolid=128.8;"      // 2300*700*8e-5 (rho * cp * kappa)
);

expressions
(
    lambda_reset
    {
        field         lambda;
        expression    "0";
    }

//     lambda_siliconLiquid
