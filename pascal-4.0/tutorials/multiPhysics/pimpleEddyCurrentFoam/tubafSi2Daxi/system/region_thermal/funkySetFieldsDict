FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      funkySetFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

materialProperties
(
    // Data from Vladi (currently nu_siliconLiquid/Pr = 3.333e-7/0.0122 [m^2/s])
    "kappa_siliconSolid=2.732e-5;"   // Conductivity of solid silicon in [m^2/s]
);

expressions
(
    kappa_reset
    {
        field         kappa;
        expression    "0";
    }

    kappa_solidSilicon
    {
        field         kappa;
        variables     $materialProperties;
        expression    "kappa_siliconSolid";
        condition     "set(cellSet_material_siliconSolid)";
    }
);

