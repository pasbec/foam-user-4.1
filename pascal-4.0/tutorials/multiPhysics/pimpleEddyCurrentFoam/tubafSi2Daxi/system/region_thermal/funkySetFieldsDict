    lambda_siliconSolid
    {
        field         lambda;
        variables     $materialProperties;
        expression    "lambda_siliconSolid";
        condition     "set(cellSet_material_siliconSolid)";
    }

    rhoCp_reset
    {
        field         rhoCp;
        expression    "0";
    }

//     rhoCp_siliconLiquid
