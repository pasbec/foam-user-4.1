FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default                 none;
}

gradSchemes
{
    default                 none;

    grad(psi)               Gauss linear;
}

divSchemes
{
    default                 none;
}

laplacianSchemes
{
    default                 none;
//     laplacian(psiGamma,psi) Gauss linear uncorrected; // linear is not used
//     laplacian(psiGamma,psi) Gauss linear corrected; // linear is not used
//     laplacian(psiGamma,psi) Gauss linear limited 0.5; // linear is not used
//     laplacian(psiGamma,psi) Gauss linear limited 0.3; // linear is not used
//     laplacian(psiGamma,psi) Gauss linear fourth; // linear is not used
//     laplacian(psiGamma,psi) Gauss linear skewCorrected 1.0; // linear is not used
//     laplacian(psiGamma,psi) Gauss linear fourth; // linear is not used
    laplacian(psiGamma,psi) Gauss linear corrected; // linear is not used
}

interpolationSchemes
{
    default                 none;

    interpolate(grad(psi))  linear;
}

fluxRequired
{
    default                 no;
    psi                     ;
}

