FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default                           none;
}

gradSchemes
{
    default                           none;

    curl(A0Re)                        Gauss linear;
    curl(A0Im)                        Gauss linear;

    snGradCorr(A0Re)                  Gauss linear;
    snGradCorr(A0Im)                  Gauss linear;

//     grad(sigma)                       Gauss linear;
//     grad(sigma)                       Gauss harmonic;

//     grad(VRe)                         Gauss linear;
//     grad(VIm)                         Gauss linear;
    grad(VRe)                         electricPotentialGrad Gauss sigma ddtAImPhi;
    grad(VIm)                         electricPotentialGrad Gauss sigma ddtARePhi;
//
//     snGradCorr(VRe)                   Gauss linear;
//     snGradCorr(VIm)                   Gauss linear;
}

divSchemes
{
    default                           none;
}

laplacianSchemes
{
    default                           none;

    laplacian(A0Re)                   Gauss linear corrected;
    laplacian(A0Im)                   Gauss linear corrected;

//     laplacian(sigma,VRe)              Gauss linear corrected;
//     laplacian(sigma,VIm)              Gauss linear corrected;
//     laplacian(sigma,VRe)              Gauss linear uncorrected;
//     laplacian(sigma,VIm)              Gauss linear uncorrected;
    laplacian(sigma,VRe)              electricPotentialLaplacian Gauss ddtAImPhi;
    laplacian(sigma,VIm)              electricPotentialLaplacian Gauss ddtARePhi;
}

interpolationSchemes
{
    default                           none;

    interpolate(A0Re)                 linear;
    interpolate(A0Im)                 linear;

    interpolate(ARe)                  linear;
    interpolate(AIm)                  linear;
}

// ************************************************************************* //
