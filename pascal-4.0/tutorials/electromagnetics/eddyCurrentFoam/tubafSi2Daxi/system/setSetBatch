### Geometry

# All

cellSet  cellSet_geometry_all             new    boxToCell      (-10 -10 -10) ( 10  10  10)

# Fluid

pointSet pointSet_geometry_fluid          new    surfaceToPoint "constant/triSurface/geometry_region_fluid.stl" 2e-4 true false

cellSet  cellSet_geometry_fluid           new    pointToCell    pointSet_geometry_fluid all

pointSet pointSet_geometry_fluid          remove

# Solid

pointSet pointSet_geometry_solid          new    surfaceToPoint "constant/triSurface/geometry_region_solid.stl" 2e-4 true false

cellSet  cellSet_geometry_solid           new    pointToCell    pointSet_geometry_solid all

pointSet pointSet_geometry_solid          remove

# Heater

# TODO: Why is non-2D version constant/triSurface/geometry_region_heater.stl not working
pointSet pointSet_geometry_heater          new    surfaceToPoint "constant/triSurface/geometry_region_heater_2D.stl" 2e-4 true false

cellSet  cellSet_geometry_heater           new    pointToCell    pointSet_geometry_heater all

pointSet pointSet_geometry_heater          remove

# Conductor

# TODO: Why is non-2D version constant/triSurface/geometry_region_conductor.stl not working
pointSet pointSet_geometry_conductor      new    surfaceToPoint "constant/triSurface/geometry_region_conductor_2D.stl" 2e-4 true false

cellSet  cellSet_geometry_conductor       new    pointToCell    pointSet_geometry_conductor all

pointSet pointSet_geometry_conductor      remove

# Background

cellSet  cellSet_geometry_background      new    setToCell      cellSet_geometry_all
cellSet  cellSet_geometry_background      delete setToCell      cellSet_geometry_conductor
cellSet  cellSet_geometry_background_old  remove



### Outer/Inner boundaries

# Outer face sets

pointSet pointSet_patch_infinity          new    surfaceToPoint "constant/triSurface/geometry_patch_infinity.stl" 2e-4 false false

faceSet  faceSet_patch_infinity           new    pointToFace    pointSet_patch_infinity all

# Inner face sets



### Materials

cellSet cellSet_material_silicon_liquid   new    setToCell      cellSet_geometry_fluid

cellSet cellSet_material_silicon_solid    new    setToCell      cellSet_geometry_solid

cellSet cellSet_material_graphite         new    setToCell      cellSet_geometry_heater

cellSet cellSet_material_air              new    setToCell      cellSet_geometry_background



### Regions

cellSet cellSet_region_conductor          new    setToCell      cellSet_geometry_conductor

# TODO: Remove front and back
faceSet faceSet_regions                   new    cellToFace     cellSet_region_conductor all
faceSet faceSet_regions                   delete cellToFace     cellSet_region_conductor both
faceSet faceSet_regions_old               remove



### End

quit
