### Regions

cellSet  cellSet_region_all               new    boxToCell      (-10 -10 -10) ( 10  10  10)

pointSet pointSet_region_fluid            new    surfaceToPoint "constant/triSurface/geometry_region_fluid.stl" 2e-4 true false
pointSet pointSet_region_solid            new    surfaceToPoint "constant/triSurface/geometry_region_solid.stl" 2e-4 true false
pointSet pointSet_region_heater           new    surfaceToPoint "constant/triSurface/geometry_region_heater.stl" 2e-4 true false

cellSet  cellSet_region_fluid             new    pointToCell    pointSet_region_fluid all
cellSet  cellSet_region_solid             new    pointToCell    pointSet_region_solid all
cellSet  cellSet_region_heater            new    pointToCell    pointSet_region_heater all

pointSet pointSet_region_fluid            remove
pointSet pointSet_region_solid            remove
pointSet pointSet_region_heater           remove

#

cellSet  cellSet_region_static            new    setToCell      cellSet_region_all
cellSet  cellSet_region_static            delete setToCell      cellSet_region_fluid
cellSet  cellSet_region_static_old        remove

cellSet  cellSet_region_conductor         new    setToCell      cellSet_region_fluid
cellSet  cellSet_region_conductor         add    setToCell      cellSet_region_solid
cellSet  cellSet_region_conductor         add    setToCell      cellSet_region_heater
cellSet  cellSet_region_conductor_old     remove

cellSet  cellSet_region_space             new    setToCell      cellSet_region_all
cellSet  cellSet_region_space             delete setToCell      cellSet_region_conductor
cellSet  cellSet_region_space_old         remove

### Materials

cellSet cellSet_material_siliconLiquid    new    setToCell      cellSet_region_fluid
cellSet cellSet_material_siliconSolid     new    setToCell      cellSet_region_solid
cellSet cellSet_material_graphite         new    setToCell      cellSet_region_heater
cellSet cellSet_material_air              new    setToCell      cellSet_region_space

### Patches

pointSet pointSet_patch_front             new    surfaceToPoint "constant/triSurface/geometry_patch_front.stl" 2e-4 false false
pointSet pointSet_patch_back              new    surfaceToPoint "constant/triSurface/geometry_patch_back.stl" 2e-4 false false
pointSet pointSet_patch_infinity          new    surfaceToPoint "constant/triSurface/geometry_patch_infinity.stl" 2e-4 false false

faceSet  faceSet_patch_front              new    pointToFace    pointSet_patch_front all
faceSet  faceSet_patch_back               new    pointToFace    pointSet_patch_back all
faceSet  faceSet_patch_infinity           new    pointToFace    pointSet_patch_infinity all

pointSet pointSet_patch_front             remove
pointSet pointSet_patch_back              remove
pointSet pointSet_patch_infinity          remove

#

faceSet faceSet_regions1                  new    cellToFace     cellSet_region_fluid all
faceSet faceSet_regions1                  delete cellToFace     cellSet_region_fluid both
faceSet faceSet_regions1                  add    cellToFace     cellSet_region_static all
faceSet faceSet_regions1                  delete cellToFace     cellSet_region_static both
faceSet faceSet_regions1_old              remove

faceSet faceSet_regions2                  new    cellToFace     cellSet_region_conductor all
faceSet faceSet_regions2                  delete cellToFace     cellSet_region_conductor both
faceSet faceSet_regions2                  add    cellToFace     cellSet_region_space all
faceSet faceSet_regions2                  delete cellToFace     cellSet_region_space both
faceSet faceSet_regions2_old              remove

faceSet faceSet_regions                   new    setToFace      faceSet_regions1
faceSet faceSet_regions                   add    setToFace      faceSet_regions2
faceSet faceSet_regions                   delete setToFace      faceSet_patch_front
faceSet faceSet_regions                   delete setToFace      faceSet_patch_back
faceSet faceSet_regions                   delete setToFace      faceSet_patch_infinity
faceSet faceSet_regions1                  remove
faceSet faceSet_regions2                  remove
faceSet faceSet_regions_old               remove

### End

quit
